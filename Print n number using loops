#include<stdio.h>

int main()
{
    int n;
    
    printf("Enter a number :");
    scanf("%d",&n);
    
    int i=1;
    while(i<=n)
    {
        printf("%d\n",i);
        i++;
    }
    
    return 0;
}

-------------------------------------------------------------------

#include<stdio.h>

int main()
{
    int n,i;
    
    printf("Enter a number :");
    scanf("%d",&n);
    
   for(i=1;i<=n;i++)
   {
       printf("%d\n",i);
   }
    
    return 0;
}

-------------------------------------------------------------------

#include<stdio.h>

int main()
{
    int n;
    
    printf("Enter a number :");
    scanf("%d",&n);
    
    int i=1;
    do
    {
        printf("%d\n",i);
        i++;
    }
    while(i<=n);
    
    return 0;
}
