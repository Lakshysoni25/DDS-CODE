#include<stdio.h>
#include<stdlib.h>
int main()
{
    int n,i;
    
    printf("Enter the size of array : ");
    scanf("%d",&n);
    
    int*arr=(int*)malloc(n*sizeof(int));
    
    for(i=0;i<n;i++)
    {
        printf("Enter the value of arr[%d] index : ",i);
        scanf("%d",&arr[i]);
    }
    printf("All the element of array : \n");
    for(i=0;i<n;i++)
    {
        printf("%d\n",arr[i]);
    }
    return 0;
}
