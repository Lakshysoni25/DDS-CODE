#include<iostream>
using namespace std;

int main()
{
    int n,i;
    cout<<"Enter the size of array : ";
    cin>>n;
    int *arr=new int[n];
    
    for(i=0;i<n;i++)
    {
       cout<<"Enter the value at arr["<<i<<"]index : ";
       cin>>arr[i];
    }
     cout<<"the element of array is :";
     
    for(i=0;i<n;i++)
    {
      cout<<arr[i]<<endl; 
    }
    return 0;
}
